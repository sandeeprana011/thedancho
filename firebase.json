{
  "hosting": [
    {
      "target": "root",
      "public": "./public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "appAssociation": "AUTO",
      "rewrites": [
        { "source": "/link/**", "dynamicLinks": true },
        {
          "source": "**",
          "public": "./public",
          "destination": "./public/index.html"
        },
        {
          "ignore": [
            "*"
          ],
          "source": "/**",
          "dynamicLinks": true
        }
      ]
    },
    {
      "site": "whileranaq",
      "public": "./public/while",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**",
        "fm","fmradio","write"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "./public/"
        }
      ]
    }
  ]
}
